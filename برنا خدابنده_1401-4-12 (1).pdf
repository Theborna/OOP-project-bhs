  <script>
    document.addEventListener('DOMContentLoaded', function () {
      // For Showing Password
      var passwordInput = document.getElementById("password-input")
      $('#show-password').checkbox({
        onChecked: function () {
          passwordInput.type="text"
        },
        onUnchecked: function () {
          passwordInput.type="password"
        }
      })

      $('.ui.form').form({
        inline: true,
        fields: {
          login: {
            identifier: 'login',
            rules: [
              {type: 'empty', prompt: 'لطفا آدرس ایمیل خود را وارد کنید.'},
              {type: 'email', prompt: 'لطفا آدرس ایمیل معتبر وارد کنید.'}
            ]
          },
          password: {
            identifier: 'password',
            rules: [
              {type: 'empty', prompt: 'لطفا گذرواژه خود را وارد کنید.'}
            ]
          }
        }
      });
    });
